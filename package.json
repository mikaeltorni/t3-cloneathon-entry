{
  "name": "openrouter-chat-app",
  "version": "1.0.0",
  "description": "Chat application with OpenRouter API integration",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run web:dev\"",
    "server:dev": "ts-node src/server/index.ts",
    "web:dev": "cd web-app && npm run dev",
    "build": "npm run server:build && npm run web:build",
    "server:build": "tsc -p tsconfig.server.json",
    "web:build": "cd web-app && npm run build",
    "start": "node dist/server/index.js",
    "image": "ts-node src/cli/index.ts",
    "text": "ts-node src/cli/index.ts --text"
  },
  "keywords": ["typescript", "openrouter", "gemini", "image-analysis", "ai"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1"
  }
} 